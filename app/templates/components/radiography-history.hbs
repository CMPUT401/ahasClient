<div class = "new-entry-button-wrapper">
	<button {{action "uploadResult" }} class = "new-entry-button">Upload Radiograph</button>
</div>	
<div class = "infoBlock">
	<div class = "panel panel-default">		
		<div class="history-list">
		{{#ember-scrollable autoHide=false}}
				{{#each radiographyList as |radiograph|}}
					<div class = "nameListItem" {{action 'viewEntry' radiograph.imageId}}>
						
							<p>{{radiograph.name}} {{radiograph.date}}</p>
						
					</div>
				{{/each}}
			{{/ember-scrollable}}
		</div>
	</div>
</div>