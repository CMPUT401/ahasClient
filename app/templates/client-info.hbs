<div class = "panel panel-default">
		<div class = "panel-body">
			<div class = "infoBlock">
				<h4>Client: </h4><br>
				<p>{{model.lastName}}, {{model.firstName}}  </p><br>
				<h5>Address:</h5><br>
				<p>{{model.address}} </p><br>
				<h5>Phone Number: </h5><br>
				<p>{{model.phoneNumber}} </p><br>
				<h5>Email: </h5><br>
				<p>{{model.email}} </p><br>
			</div>
			<div class = "infoBlock">
				<h4>Documentation Information</h4><br>
				<h5>LICO: </h5><br>
				<p>{{model.licos}}</p><br>
				<h5>AISH: </h5><br>
				<p>{{model.aish}}</p><br>
				<h5>SA: </h5><br>
				<p>{{model.socialAssistance}}</p><br>
				<h5>Notes: </h5><br>
				<p>{{model.notes}}</p><br>
			</div>
			<div class = "infoBlock">
				<h4>Alternate Contact</h4><br>
				<p>{{model.alternativeContactFirstName}}</p><br>
				<h5>Last, First</h5><br>
				<p>{{model.alternativeContactLastName}}</p><br>
				<h5>Address: </h5><br>
				<p>{{model.alternativeContactAddress}}</p><br>
				<h5>Phone Number: </h5><br>
				<p>{{model.alternativeContactPhoneNumber}}</p><br>
				<h5>Second Phone Number: </h5>
				<p>{{model.alternativeContact2ndPhone}}</p><br>
				<h5>Email: </h5><br>
				<p>{{model.alternativeContactEmail}}</p><br>
			</div>
			<div class = "infoBlock">
				<h4>Pet: </h4>
				<!-- TODO loop through list of pets -->
				<h5>Pet name</h5><br>
				<p>{{model.pets}}</p><br>
				<h5>Species: </h5><br>
				<h5>Animal ID: </h5><br>
				<h5>Tattoo/microchip ID: </h5><br>
			</div>
			<h5>Created: </h5><p>{{model.created_at}}</p><br>
			<h5>Last modified: </h5><p>{{model.updated_at}}</p><br>
		</div>
</div>
