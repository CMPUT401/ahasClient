 <div class="inputContactGroup">
 <h2> New contact: </h2>
 <label for="type" >What type of volunteer is this?</label>
  <br>
  <br>
  <select id="type" {{action "showLastName" on="change"}} >
        <option value="Veterinarian" > Veterinarian </option>
        <option value="Volunteer" > Volunteer </option>
        <option value="Laboratory" > Laboratory </option>
         <option value="Laboratory" >Technician </option>
  </select>
<form > 
  <br>
  <br>    
  {{#if model.laboratory}}
  <label for="first_name" class = "inputContactLabel">Name:</label>
 
  {{input id='first_name' class = "inputContactBox" value=first_name }}
  <br>
  <br> 
  {{else}}
  <label for="first_name" class = "inputContactLabel">First Name:</label>
 
  {{input id='first_name' class = "inputContactBox" value=first_name }}
  <br>
  <br> 
  <label for="last_name" class="inputContactLabel">Last Name:</label>
 
  {{input id='last_name' class="inputContactBox" value=last_name }}
  <br>
  <br> 
  {{/if}}
  <label for="phoneNumber" class="inputContactLabel">Phone Number: </label>
  
  {{input id='phoneNumber' class="inputContactBox" value=phoneNumber }}
  <br>
  <br>
  <label for="faxNumber" class="inputContactLabel">Fax Number: </label>

  {{input type="faxNumber" id='faxNumber' class="inputContactBox" value=faxNumber }}
  <br>
  <br>
  <label for="email" class="inputContactLabel">Email: </label>
 
  {{input type="email" id='email' class="inputContactBox" value=email }}
  <br>
  <br>
  <label for="address" class="inputContactLabel">Address: </label>
 
  {{input type="address" id='address' class="inputContactBox" value=address }}
  <br>
  <br>
  <br>
  <br>
  <button type="submit" {{action 'createContact'}} id="create-contact-button">Create</button>
 </form>
 <br>
 <div id = "alert_placeholder"></div>
 </div>